	</action>	
	
	<scene name="scene_Museum_Perjuangan_1" title="" onstart="" thumburl="panos/Museum_Perjuangan_1.tiles/thumb.jpg" lat="" lng="" heading="">

		<view hlookat="0" vlookat="0" fovtype="MFOV" fov="120" maxpixelzoom="2.0" fovmin="70" fovmax="140" limitview="auto" />

		<preview url="panos/Museum_Perjuangan_1.tiles/preview.jpg" />

		<image>
			<cube url="panos/Museum_Perjuangan_1.tiles/mobile_%s.jpg" />
			<mobile>
				<cube url="panos/Museum_Perjuangan_1.tiles/mobile_%s.jpg" />
			</mobile>
		</image>

		<!-- place your scene hotspots here -->

	</scene>

